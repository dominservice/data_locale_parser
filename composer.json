{
    "name": "dominservice/data_locale_parser",
    "type": "library",
    "description": "Data Locale Parser is a package for Laravel 5.6 | 5.7 | 5.8 | 6.* | 7.* | 8.* | 9.*, which lists all countries, currencies and languages, with names and ISO 3166 codes in all languages and data formats.",
    "keywords": ["laravel", "php", "currencies", "languages", "countries", "data parser", "country list", "currencies list", "languages list", "umpirsky"],
    "homepage": "https://github.com/dominservice/data_locale_parser",
    "license": "MIT",
    "authors": [
        {
            "name": "dominservice",
            "email": "biuro@dso.biz.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "laravel/framework": "^5.6|^5.7|^5.8|^6|^7|^8|^9"
    },
    "require-all": true,
    "autoload": {
        "psr-4": {
            "Dominservice\\DataLocaleParser\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev"
}
